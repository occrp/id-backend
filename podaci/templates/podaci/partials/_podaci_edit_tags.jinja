<div class="modal" id="podaci_edit_tags_modal">
  <div class="modal-dialog">
    <div class="modal-content">
      <div class="modal-header">
        <button type="button" class="close" data-dismiss="modal" aria-label="Close">
          <span aria-hidden="true">&times;</span></button>
        <h4 class="modal-title">{% trans %}Edit tags{% endtrans %}</h4>
      </div>
      <div class="modal-body">
        <select class="span5" id="podaci_edit_tags_input" multiple="true">
        </select>
      </div>
      <div class="modal-footer">
        <button type="button" class="btn btn-default" id="podaci_edit_tags_btn">Save</button>
      </div>
    </div><!-- /.modal-content -->
  </div><!-- /.modal-dialog -->
</div>
